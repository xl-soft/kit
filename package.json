{
	"name": "test",
	"version": "0.1.0",
	"scripts": {
		"dev": "vite dev",
		"start": "docker-compose -f ./build/docker-compose.yaml up",
		"build": "concurrently -m 1 \"docker network create net\" \"docker image prune -f\" \"vite build\" \"npm run compose\" \"docker-compose -f ./build/docker-compose.yaml build\" \"docker image prune -f\"",
		"compile": "concurrently -m 1 \"npm run build\" \"docker-compose -f ./build/docker-compose.yaml up\"",
		"compose": "concurrently -m 1 \"tsc ./config/build.ts --target es2022 --moduleResolution node\" \"node -r dotenv/config ./config/build.js\"",
		"del": "del --force \"./temp\" "
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.106",
		"@sveltejs/kit": "^1.5.0",
		"concurrently": "^7.6.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"del-cli": "^5.0.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"js-yaml": "^4.1.0",
		"mongoose": "^7.0.1",
		"systemjs": "^6.14.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.2"
	}
}
